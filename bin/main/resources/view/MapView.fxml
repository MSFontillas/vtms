<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.controller.MapViewController">
    <center>
        <TableView fx:id="mapTable" stylesheets="@styles/TableStyles.css" BorderPane.alignment="CENTER">
            <columns>
                <TableColumn fx:id="mapID" editable="false" minWidth="100.0" text="ID" />
                <TableColumn fx:id="mapName" editable="false" minWidth="200.0" text="Name" />
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
        </TableView>
    </center>
</BorderPane>
